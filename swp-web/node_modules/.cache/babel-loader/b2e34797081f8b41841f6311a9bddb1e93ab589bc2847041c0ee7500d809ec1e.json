{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport pay from \"@/components/AppPay.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"AppUser\",\n  components: {\n    pay\n  },\n  data() {\n    return {\n      showRecharge: false,\n      name: '',\n      NAME: '',\n      BALANCE: '',\n      USERID: '',\n      PASSWORD: '',\n      userchoseshow: '',\n      images: [],\n      srcurl: '',\n      usechose: {\n        t1: '',\n        t2: '',\n        t3: '',\n        t4: '',\n        t5: ''\n      }\n    };\n  },\n  mounted() {\n    // 在组件加载时自动执行的函数\n\n    this.yourFunction();\n    this.NAME = sessionStorage.getItem('NAME');\n    this.USERID = sessionStorage.getItem('USERID');\n    this.PASSWORD = sessionStorage.getItem('PASSWORD');\n    this.login();\n    this.BALANCE = sessionStorage.getItem('BALANCE');\n    this.userchoseshow = sessionStorage.getItem('USERCHOSE');\n    sessionStorage.setItem('SEARCHTYPE', '');\n    sessionStorage.setItem('LASTURL', '/user');\n    console.log(this.NAME);\n    console.log(this.USERID);\n    console.log(this.PASSWORD);\n    console.log(this.BALANCE);\n    this.postImageName();\n    this.usechose = {\n      t1: '',\n      t2: '',\n      t3: '',\n      t4: '',\n      t5: ''\n    };\n    if (this.userchoseshow == 'myup') {\n      this.usechose.t1 = 'cho';\n    }\n    if (this.userchoseshow == 'mybuy') {\n      this.usechose.t2 = 'cho';\n    }\n    if (this.userchoseshow == 'myload') {\n      this.usechose.t3 = 'cho';\n    }\n    if (this.userchoseshow == 'mylike') {\n      this.usechose.t4 = 'cho';\n    }\n    if (this.userchoseshow == 'mycollect') {\n      this.usechose.t5 = 'cho';\n    }\n  },\n  methods: {\n    myupchose() {\n      sessionStorage.setItem('USERCHOSE', 'myup');\n      location.reload();\n    },\n    mybuychose() {\n      sessionStorage.setItem('USERCHOSE', 'mybuy');\n      location.reload();\n    },\n    myloadchose() {\n      sessionStorage.setItem('USERCHOSE', 'myload');\n      location.reload();\n    },\n    mycollectchose() {\n      sessionStorage.setItem('USERCHOSE', 'mycollect');\n      location.reload();\n    },\n    mylikechose() {\n      sessionStorage.setItem('USERCHOSE', 'mylike');\n      location.reload();\n    },\n    showRechargeBox() {\n      //显示充值\n      this.showRecharge = true;\n    },\n    topicture(picture, picid) {\n      this.picturesrc = picture;\n      this.pictureid = picid;\n      console.log(this.pictureid);\n      this.$router.push({\n        path: '/picture',\n        query: {\n          name: this.picturesrc,\n          wpid: this.pictureid\n        }\n      });\n    },\n    tologin() {\n      sessionStorage.setItem('NAME', '');\n      sessionStorage.setItem('USERID', '');\n      sessionStorage.setItem('BALANCE', '');\n      sessionStorage.setItem('PASSWORD', '');\n      this.$router.push(\"/\");\n    },\n    yourFunction() {\n      console.log(\"hhhhh\");\n    },\n    toindex() {\n      this.$router.push(\"/index\");\n    },\n    refresh() {\n      location.reload();\n    },\n    toupload() {\n      this.$router.push(\"/upload\");\n    },\n    topay() {\n      this.$router.push(\"/pay\");\n    },\n    async postImageName() {\n      let self = this;\n      let vwallpaper = {};\n      if (this.userchoseshow == 'myup') {\n        vwallpaper = {\n          vwpId: '',\n          name: '',\n          createId: this.USERID,\n          price: '',\n          type: '',\n          path: ''\n        };\n        this.srcurl = \"http://localhost:8090/vwallpaper/getImagesByCreateId\";\n      }\n      if (this.userchoseshow == 'mybuy') {\n        vwallpaper = {\n          vwpId: '',\n          userId: this.USERID\n        };\n        this.srcurl = \"http://localhost:8090/vwporder/getPurchasedImages\";\n      }\n      if (this.userchoseshow == 'myload') {\n        vwallpaper = {\n          vwpId: '',\n          userId: this.USERID\n        };\n        this.srcurl = \"http://localhost:8090/vwpdownload/getDownloadedImages\";\n      }\n      if (this.userchoseshow == 'mycollect') {\n        vwallpaper = {\n          vwpId: '',\n          userId: this.USERID\n        };\n        this.srcurl = \"http://localhost:8090/vwporder/getPurchasedImages\";\n      }\n      if (this.userchoseshow == 'mylike') {\n        vwallpaper = {\n          vwpId: '',\n          userId: this.USERID\n        };\n        this.srcurl = \"http://localhost:8090/myfavourite/favourite\";\n      }\n      if (this.userchoseshow == 'mycollect') {\n        vwallpaper = {\n          vwpId: '',\n          userId: this.USERID\n        };\n        this.srcurl = \"http://localhost:8090/mycollect/collect\";\n      }\n      try {\n        //使用axios发送一个GET请求，假设后端有一个接口可以返回图片的文件名\n        let response = await axios.post(this.srcurl, vwallpaper);\n        //如果请求成功，将文件名保存到imageName属性中\n        if (response.status === 200) {\n          console.log(response.data);\n          console.log(this.imageName);\n          let i = 0;\n          while (response.data.data[i] && response.data.data[i].path != null) {\n            this.images.push({\n              imageNames: response.data.data[i].path,\n              imageID: response.data.data[i].vwpId\n            });\n            i++;\n          }\n        }\n      } catch (error) {\n        //如果请求失败，打印错误信息\n        console.error(error);\n      }\n    },\n    login() {\n      let user = {\n        userId: this.USERID,\n        password: this.PASSWORD\n      };\n      let self = this;\n      this.$axios.post('http://localhost:8090/user/login', user) ////////////////////////////////////登录信息\n      .then(function (response) {\n        let result = response.data;\n        if (result.code === 200) {\n          console.log(\"登录成功！\");\n          console.log(\"返回的数据：\", result.data);\n          console.log(\"总记录数：\", result.total);\n          sessionStorage.setItem('NAME', result.data.name);\n          sessionStorage.setItem('USERID', result.data.userId);\n          sessionStorage.setItem('BALANCE', result.data.balance);\n          sessionStorage.setItem('PASSWORD', result.data.password);\n          sessionStorage.setItem('SEARCHNAME', '');\n          sessionStorage.setItem('SEARCHTYPE', '');\n          sessionStorage.setItem('USERCHOSE', 'myup');\n          sessionStorage.setItem('LASTURL', '');\n        } else if (result.code === 400) {\n          console.log(\"登录失败！\");\n          alert(\"账号或密码错误，请重新输入\");\n          location.reload();\n        }\n      }).catch(function (error) {\n        console.log(\"请求错误：\", error);\n      });\n    },\n    getImageUrl(imageName) {\n      return `http://localhost:8090/vwallpaper/ShowPicture?filepath=${imageName}`;\n    }\n  }\n};","map":{"version":3,"names":["pay","axios","name","components","data","showRecharge","NAME","BALANCE","USERID","PASSWORD","userchoseshow","images","srcurl","usechose","t1","t2","t3","t4","t5","mounted","yourFunction","sessionStorage","getItem","login","setItem","console","log","postImageName","methods","myupchose","location","reload","mybuychose","myloadchose","mycollectchose","mylikechose","showRechargeBox","topicture","picture","picid","picturesrc","pictureid","$router","push","path","query","wpid","tologin","toindex","refresh","toupload","topay","self","vwallpaper","vwpId","createId","price","type","userId","response","post","status","imageName","i","imageNames","imageID","error","user","password","$axios","then","result","code","total","balance","alert","catch","getImageUrl"],"sources":["src/components/AppUser.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"header\">\r\n      <div class=\"logo\">壁纸引擎--用户</div>\r\n      <div class=\"nav\">\r\n        <button @click=\"toindex\">首页</button>\r\n        <button @click=\"refresh\">刷新</button>\r\n        <button @click=\"toupload\">上传</button>\r\n        <div class=\"userimgbox\">\r\n          <img src=\"../assets/bg.jpg\" alt=\"\" width=\"25\" @click=\"refresh\">\r\n          <div style=\" color: #28e1f8;\">{{ this.NAME }}</div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"paybox\">\r\n      <pay v-if=\"showRecharge\" />\r\n    </div>\r\n    <div class=\"profile\">\r\n      <img src=\"../assets/bg.jpg\">\r\n      <div class=\"profile-info\">\r\n        <p>用户名: <span>{{ this.NAME }}</span></p>\r\n        <p>账号: <span>{{ this.USERID }}</span></p>\r\n        <p>余额: <span>￥{{ this.BALANCE }}</span></p>\r\n      </div>\r\n    </div>\r\n    <div class=\"tabs\">\r\n      <button :class=\"this.usechose.t1\" @click=\"myupchose\">我上传的壁纸</button>\r\n      <button :class=\"this.usechose.t2\" @click=\"mybuychose\">我购买的壁纸</button>\r\n      <button :class=\"this.usechose.t3\" @click=\"myloadchose\">我下载的壁纸</button>\r\n      <button :class=\"this.usechose.t4\" @click=\"mylikechose\">我点赞的壁纸</button>\r\n      <button :class=\"this.usechose.t5\" @click=\"mycollectchose\">我收藏的壁纸</button>\r\n      <button class=\"active\" @click=\"toupload\">上传壁纸</button>\r\n      <button class=\"active\" @click=\"showRechargeBox\">充值</button>\r\n      <button class=\"active\" @click=\"tologin\">退出登录</button>\r\n    </div>\r\n    <div class=\"imggallery\">\r\n      <div class=\"image-viewer\">\r\n        <el-image class=\"image-style\" v-for=\"(aimage, index) in images\" :key=\"index\" :src=\"getImageUrl(aimage.imageNames)\" fit=\"contain\" @click=\"topicture(getImageUrl(aimage.imageNames),aimage.imageID)\"></el-image>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"footer\">\r\n      <p>© 2023 壁纸引擎. All rights reserved.</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport pay from \"@/components/AppPay.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"AppUser\",\r\n  components: {pay,},\r\n  data() {\r\n    return {\r\n      showRecharge: false,\r\n      name: '',\r\n      NAME:'',\r\n      BALANCE:'',\r\n      USERID:'',\r\n      PASSWORD:'',\r\n      userchoseshow:'',\r\n      images:[],\r\n      srcurl:'',\r\n      usechose: {\r\n        t1:'',\r\n        t2:'',\r\n        t3:'',\r\n        t4:'',\r\n        t5:'',\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    // 在组件加载时自动执行的函数\r\n\r\n    this.yourFunction();\r\n    this.NAME=sessionStorage.getItem('NAME')\r\n    this.USERID=sessionStorage.getItem('USERID')\r\n    this.PASSWORD=sessionStorage.getItem('PASSWORD')\r\n    this.login();\r\n    this.BALANCE=sessionStorage.getItem('BALANCE')\r\n    this.userchoseshow=sessionStorage.getItem('USERCHOSE')\r\n    sessionStorage.setItem('SEARCHTYPE', '')\r\n    sessionStorage.setItem('LASTURL', '/user')\r\n    console.log(this.NAME)\r\n    console.log(this.USERID)\r\n    console.log(this.PASSWORD)\r\n    console.log(this.BALANCE)\r\n    this.postImageName();\r\n    this.usechose={\r\n      t1:'',\r\n      t2:'',\r\n      t3:'',\r\n      t4:'',\r\n      t5:'',\r\n    }\r\n    if(this.userchoseshow=='myup')\r\n    {this.usechose.t1='cho'}\r\n    if(this.userchoseshow=='mybuy')\r\n    {this.usechose.t2='cho'}\r\n    if(this.userchoseshow=='myload')\r\n    {this.usechose.t3='cho'}\r\n    if(this.userchoseshow=='mylike')\r\n    {this.usechose.t4='cho'}\r\n    if(this.userchoseshow=='mycollect')\r\n    {this.usechose.t5='cho'}\r\n\r\n  },\r\n  methods: {\r\n    myupchose(){\r\n      sessionStorage.setItem('USERCHOSE', 'myup');\r\n      location.reload();\r\n    },\r\n    mybuychose(){\r\n      sessionStorage.setItem('USERCHOSE', 'mybuy');\r\n      location.reload();\r\n    },\r\n    myloadchose(){\r\n      sessionStorage.setItem('USERCHOSE', 'myload');\r\n      location.reload();\r\n    },\r\n    mycollectchose(){\r\n      sessionStorage.setItem('USERCHOSE', 'mycollect');\r\n      location.reload();\r\n    },\r\n    mylikechose(){\r\n      sessionStorage.setItem('USERCHOSE', 'mylike');\r\n      location.reload();\r\n    },\r\n    showRechargeBox() {    //显示充值\r\n      this.showRecharge = true;\r\n    },\r\n    topicture(picture,picid) {\r\n      this.picturesrc = picture;\r\n      this.pictureid=picid;\r\n      console.log(this.pictureid)\r\n      this.$router.push({ path: '/picture', query: { name: this.picturesrc, wpid: this.pictureid} });\r\n    },\r\n    tologin(){\r\n      sessionStorage.setItem('NAME', '')\r\n      sessionStorage.setItem('USERID', '')\r\n      sessionStorage.setItem('BALANCE', '')\r\n      sessionStorage.setItem('PASSWORD', '')\r\n      this.$router.push(\"/\");\r\n    },\r\n    yourFunction() {\r\n      console.log(\"hhhhh\");\r\n    },\r\n    toindex() {\r\n      this.$router.push(\"/index\");\r\n    },\r\n    refresh() {\r\n      location.reload();\r\n    },\r\n    toupload() {\r\n      this.$router.push(\"/upload\");\r\n    },\r\n    topay(){\r\n      this.$router.push(\"/pay\");\r\n    },\r\n    async postImageName() {\r\n      let self = this;\r\n      let vwallpaper={};\r\n\r\n      if(this.userchoseshow=='myup') {\r\n        vwallpaper = {\r\n\r\n          vwpId: '',\r\n          name: '',\r\n          createId: this.USERID,\r\n          price: '',\r\n          type: '',\r\n          path: '',\r\n        }\r\n        this.srcurl = \"http://localhost:8090/vwallpaper/getImagesByCreateId\";\r\n      }\r\n      if(this.userchoseshow=='mybuy') {\r\n        vwallpaper = {\r\n\r\n          vwpId: '',\r\n          userId: this.USERID,\r\n        }\r\n        this.srcurl = \"http://localhost:8090/vwporder/getPurchasedImages\";\r\n      }\r\n        if(this.userchoseshow=='myload') {\r\n          vwallpaper = {\r\n\r\n            vwpId: '',\r\n            userId: this.USERID,\r\n          }\r\n          this.srcurl = \"http://localhost:8090/vwpdownload/getDownloadedImages\";\r\n        }\r\n      if(this.userchoseshow=='mycollect') {\r\n        vwallpaper = {\r\n\r\n          vwpId: '',\r\n          userId: this.USERID,\r\n        }\r\n        this.srcurl = \"http://localhost:8090/vwporder/getPurchasedImages\";\r\n      }\r\n      if(this.userchoseshow=='mylike') {\r\n        vwallpaper = {\r\n\r\n          vwpId: '',\r\n          userId: this.USERID,\r\n        }\r\n        this.srcurl = \"http://localhost:8090/myfavourite/favourite\";\r\n      }\r\n      if(this.userchoseshow=='mycollect') {\r\n        vwallpaper = {\r\n\r\n          vwpId: '',\r\n          userId: this.USERID,\r\n        }\r\n        this.srcurl = \"http://localhost:8090/mycollect/collect\";\r\n      }\r\n      try {\r\n        //使用axios发送一个GET请求，假设后端有一个接口可以返回图片的文件名\r\n        let response = await axios.post(this.srcurl,vwallpaper);\r\n        //如果请求成功，将文件名保存到imageName属性中\r\n        if (response.status === 200) {\r\n          console.log(response.data);\r\n          console.log(this.imageName);\r\n          let i=0;\r\n          while(response.data.data[i] && response.data.data[i].path!=null){\r\n            this.images.push({imageNames:response.data.data[i].path,imageID:response.data.data[i].vwpId})\r\n            i++;\r\n          }\r\n        }\r\n      } catch (error) {\r\n        //如果请求失败，打印错误信息\r\n        console.error(error);\r\n      }\r\n    },\r\n    login() {\r\n      let user = {\r\n        userId: this.USERID,\r\n        password: this.PASSWORD\r\n      };\r\n      let self = this;\r\n      this.$axios.post('http://localhost:8090/user/login', user)     ////////////////////////////////////登录信息\r\n          .then(function (response) {\r\n            let result = response.data;\r\n            if (result.code === 200) {\r\n              console.log(\"登录成功！\");\r\n              console.log(\"返回的数据：\", result.data);\r\n              console.log(\"总记录数：\", result.total);\r\n\r\n              sessionStorage.setItem('NAME', result.data.name)\r\n              sessionStorage.setItem('USERID', result.data.userId)\r\n              sessionStorage.setItem('BALANCE', result.data.balance)\r\n              sessionStorage.setItem('PASSWORD', result.data.password)\r\n              sessionStorage.setItem('SEARCHNAME', '')\r\n              sessionStorage.setItem('SEARCHTYPE', '')\r\n              sessionStorage.setItem('USERCHOSE', 'myup')\r\n   